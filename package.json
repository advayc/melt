{
  "name": "screen-time-tracker",
  "version": "0.1.0",
  "description": "Electron + React macOS screen time tracker with per-app usage and icons.",
  "main": "electron/main.js",
  "scripts": {
    "dev": "concurrently -k \"npm:dev:renderer\" \"npm:dev:electron\"",
    "dev:renderer": "vite",
    "dev:electron": "wait-on tcp:5173 && electron .",
    "build:renderer": "vite build",
    "build": "npm run build:renderer",
    "start": "electron ."
  },
  "dependencies": {
    "active-win": "^9.0.0",
    "dayjs": "^1.11.10",
    "electron": "^30.0.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.0.0",
    "concurrently": "^8.2.0",
    "vite": "^5.2.0",
    "wait-on": "^7.0.1"
  },
  "author": "",
  "license": "MIT"
}
